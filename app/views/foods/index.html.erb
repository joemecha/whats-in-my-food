<h1>Foods Search Result</h1>

<b>Total items returned: <%="#{@num_results}"%></b>
<% binding.pry %>
<h3>Foods containing <%=@query%></h3>
<% @foods[0..9].each do |f| %>
<ul>
  <li> GTIN/UPC code: <%= "#{@f.upc_code}"%>
  <li> Description: <%= "#{@f.description}"%>
  <li> Brand Owner: <%= "#{@f.brand_owner}"%>
  <li> Ingredients: <%= "#{@f.ingredients}"%>
</ul>
<%end%>